<%= form_for(@release) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist %>
  <%= f.text_field :artist %>
  <%= f.label :label %>
  <%= f.text_field :label %>
  <%= f.label :year %>
  <%= f.text_field :year %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.submit %>
<% end %>
